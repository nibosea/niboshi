int gcd_2(int a , int b){
	int A = max(a,b); //‘å‚«‚¢‚Ù‚¤
	int B = min(a,b);//¬‚³‚¢‚Ù‚¤
	int c =-1;
	int ans = 1 ;
	while(c!=0){
		c = A % B;
		if(c==0){//B‚ªÅ‘åŒö–ñ”
			ans = B;
			break;
		}
		else{//B‘åŒö–ñ”‚Å‚Í‚È‚¢‚Ì‚ÅCA‚ğB‚ÉCB‚ğC‚É‚µ‚Ä‚à‚¤ˆê“x
			A = B; 
			B = c; 
		}
	}
	return ans;//c‚ªÅ‘åŒö–ñ” 
}
